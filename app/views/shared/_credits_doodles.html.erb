<%# Decorative SVG doodles for credits page - celebratory/trophy theme %>
<div class="absolute inset-0 pointer-events-none overflow-hidden" aria-hidden="true">
  <!-- Confetti burst - top left -->
  <svg class="doodle-decoration absolute top-[5%] left-[8%] w-16 h-16 rotate-[-10deg]" viewBox="0 0 64 64" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round">
    <path d="M32 8v12M32 44v12" class="text-accent-tertiary"/>
    <path d="M8 32h12M44 32h12" class="text-accent-primary"/>
    <path d="M16 16l8 8M40 40l8 8" class="text-accent-secondary"/>
    <path d="M16 48l8-8M40 24l8-8" class="text-accent-orange"/>
    <circle cx="32" cy="32" r="3" class="text-accent-tertiary" fill="currentColor"/>
  </svg>

  <!-- Celebration swirl - top right -->
  <svg class="doodle-decoration absolute top-[10%] right-[6%] w-20 h-20 rotate-12" viewBox="0 0 80 80" fill="none" stroke="currentColor" stroke-width="2.5" stroke-linecap="round">
    <path d="M40 16c-16 0-24 12-24 24s12 24 24 24c8 0 16-4 20-12" class="text-accent-secondary"/>
    <path d="M56 36l8 12-12 4" class="text-accent-secondary"/>
  </svg>

  <!-- Trophy silhouette - bottom left -->
  <svg class="doodle-decoration absolute bottom-[12%] left-[4%] w-14 h-16 opacity-40" viewBox="0 0 56 64" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
    <path d="M16 8h24v8c0 12-4 20-12 24v8h-0c0 0 0 0 0 0v-8c-8-4-12-12-12-24V8z" class="text-accent-tertiary"/>
    <path d="M16 12H8c0 8 4 12 8 14" class="text-accent-tertiary"/>
    <path d="M40 12h8c0 8-4 12-8 14" class="text-accent-tertiary"/>
    <path d="M20 48h16" class="text-accent-tertiary"/>
    <path d="M18 52h20" class="text-accent-tertiary"/>
  </svg>

  <!-- Sparkle stars cluster - top center -->
  <svg class="doodle-decoration absolute top-[18%] left-[45%] w-12 h-12" viewBox="0 0 48 48" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round">
    <!-- 4-point star -->
    <path d="M24 8v8M24 32v8M8 24h8M32 24h8" class="text-accent-tertiary"/>
    <path d="M14 14l6 6M28 28l6 6M14 34l6-6M28 20l6-6" class="text-accent-tertiary"/>
    <!-- Mini dots -->
    <circle cx="6" cy="12" r="2" class="text-accent-primary" fill="currentColor"/>
    <circle cx="42" cy="36" r="1.5" class="text-accent-secondary" fill="currentColor"/>
  </svg>

  <!-- Confetti rectangles - right side -->
  <svg class="doodle-decoration absolute top-[40%] right-[3%] w-10 h-20" viewBox="0 0 40 80" fill="currentColor">
    <rect x="4" y="8" width="8" height="4" rx="1" class="text-accent-primary" transform="rotate(25 8 10)"/>
    <rect x="24" y="24" width="10" height="5" rx="1" class="text-accent-tertiary" transform="rotate(-15 29 26)"/>
    <rect x="8" y="48" width="6" height="3" rx="1" class="text-accent-secondary" transform="rotate(40 11 49)"/>
    <rect x="20" y="64" width="8" height="4" rx="1" class="text-accent-orange" transform="rotate(-30 24 66)"/>
  </svg>

  <!-- Wavy party streamer - bottom right -->
  <svg class="doodle-decoration absolute bottom-[18%] right-[12%] w-24 h-10 rotate-[-8deg]" viewBox="0 0 96 40" fill="none" stroke="currentColor" stroke-width="3" stroke-linecap="round">
    <path d="M4 20c12-16 24 16 36 0s24 16 36 0" class="text-accent-secondary"/>
    <path d="M8 28c10-12 20 12 30 0s20 12 30 0" class="text-accent-primary opacity-60"/>
  </svg>

  <!-- Small bursting lines - bottom center-left -->
  <svg class="doodle-decoration absolute bottom-[8%] left-[30%] w-12 h-12" viewBox="0 0 48 48" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round">
    <path d="M24 24l-12-12M24 24l12-12M24 24l-12 12M24 24l12 12M24 24v-16M24 24v16M24 24h-16M24 24h16" class="text-accent-orange"/>
  </svg>

  <!-- Floating confetti dots - scattered -->
  <svg class="doodle-decoration absolute top-[55%] left-[15%] w-6 h-6" viewBox="0 0 24 24" fill="currentColor">
    <circle cx="12" cy="12" r="3" class="text-accent-tertiary"/>
  </svg>
  <svg class="doodle-decoration absolute top-[35%] right-[20%] w-5 h-5" viewBox="0 0 20 20" fill="currentColor">
    <circle cx="10" cy="10" r="2.5" class="text-accent-green"/>
  </svg>
  <svg class="doodle-decoration absolute bottom-[30%] left-[60%] w-4 h-4" viewBox="0 0 16 16" fill="currentColor">
    <circle cx="8" cy="8" r="2" class="text-accent-indigo"/>
  </svg>
</div>
